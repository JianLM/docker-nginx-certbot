ENV_SERVICES_SERVER_NGINX_CERTBOT_CONTAINER_NAME=server-nginx-certbot
ENV_SERVICES_SERVER_NGINX_CERTBOT_IMAGE_TAG=v1.15-alpine
ENV_SERVICES_SERVER_NGINX_CERTBOT_NETWORK_MODE=host
ENV_SERVICES_SERVER_NGINX_CERTBOT_RESTART=no
ENV_SERVICES_SERVER_NGINX_CERTBOT_VOLUMES_ROOT=./../../volumes/server-nginx-certbot
ENV_SERVICES_DEFAULT_URL=http://localhost

ENV_SERVICES_SERVER_NGINX_CERTBOT_VHOSTS="MY_APP_1 MY_APP_2"

ENV_SERVICES_SERVER_NGINX_CERTBOT_VHOST_MY_APP_1_DOMAIN=app1.localhost
ENV_SERVICES_SERVER_NGINX_CERTBOT_VHOST_MY_APP_1_UPSTREAM=127.0.0.1:30080
ENV_SERVICES_SERVER_NGINX_CERTBOT_VHOST_MY_APP_1_EMAIL=admin@localhost
ENV_SERVICES_SERVER_NGINX_CERTBOT_VHOST_MY_APP_1_STAGING=virtual

ENV_SERVICES_SERVER_NGINX_CERTBOT_VHOST_MY_APP_2_DOMAIN=app2.localhost
ENV_SERVICES_SERVER_NGINX_CERTBOT_VHOST_MY_APP_2_UPSTREAM=127.0.0.1:30180
ENV_SERVICES_SERVER_NGINX_CERTBOT_VHOST_MY_APP_2_EMAIL=admin@localhost
ENV_SERVICES_SERVER_NGINX_CERTBOT_VHOST_MY_APP_2_STAGING=virtual